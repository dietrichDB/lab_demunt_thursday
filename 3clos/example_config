vlan 1000
   name clients
!
management api gnmi
   transport grpc default
!
management api netconf
   transport ssh default
!
interface Ethernet1
   switchport access vlan 1000
!
interface Ethernet2
   switchport access vlan 1000
!
interface Ethernet3
   switchport access vlan 1000
!
interface Management0
   ip address 172.20.20.2/24
   ipv6 address 3fff:172:20:20::2/64
!
interface Vlan1000
   ip address 10.0.0.100/24
